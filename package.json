{
  "name": "fema-bpm-demo",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "node dist/server.js",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "node ./node_modules/protractor/bin/webdriver-manager update",
    "e2e": "protractor",
    "build:nodeserver-dev": "ng build --env=dev && cp node-server/* dist",
    "build:nodeserver-local": "ng build --env=local && xcopy /s node-server/*.* dist",
    "build:nodeserver-prod": "ng build --env=prod && cp node-server/* dist",
    "serve-build-dev": "npm run build:nodeserver-dev && cd dist && node server.js",
    "serve-build-local": "npm run build:nodeserver-local && cd dist && node server.js",
    "serve-build-prod": "npm run build:nodeserver-prod && cd dist && node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.6",
    "@angular/compiler": "^2.4.6",
    "@angular/core": "^2.4.6",
    "@angular/forms": "^2.4.6",
    "@angular/http": "^2.4.6",
    "@angular/platform-browser": "^2.4.6",
    "@angular/platform-browser-dynamic": "^2.4.6",
    "@angular/router": "^3.4.6",
    "@swimlane/ngx-charts": "^4.1.1",
    "@swimlane/ngx-datatable": "^6.1.2",
    "@types/jquery": "^2.0.40",
    "angular2-highcharts": "^0.5.5",
    "angular2-modal": "^2.0.3",
    "angular2-select": "^1.0.0-beta.2",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "d3": "^4.6.0",
    "d3-ng2-service": "^1.6.0",
    "express": "^4.14.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "ng2-bootstrap": "^1.3.3",
    "rxjs": "^5.1.0",
    "tether": "^1.4.0",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.1",
    "@angular/compiler-cli": "^2.4.6",
    "@types/d3": "^4.5.0",
    "@types/jasmine": "2.5.41",
    "@types/jquery": "^2.0.40",
    "@types/node": "^7.0.5",
    "codelyzer": "~2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "^5.1.0",
    "ts-node": "2.0.0",
    "tslint": "^4.4.2",
    "typescript": "~2.1.5",
    "webdriver-manager": "^12.0.1"
  }
}
